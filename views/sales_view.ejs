<!DOCTYPE html>
<html>
<head>
    <!-- Add any necessary styles or scripts here -->
</head>
<body>
    <h2>Combined Data for Year <%= selectedYear %></h2>
    <table>
        <tr>
            <th>Date</th>
            <th>Gross Sales</th>
            <th>Discounts</th>
            <th>Net Sales</th>
            <th>Tax</th>
            <th>Credit Card</th>
            <th>Debit Card</th>
            <th>Cash</th>
        </tr>
        <% data.forEach(item => { %>
            <tr>
                <td><%= item.Date.toDateString() %></td>
                <td>$<%= item.GrossSales.toFixed(2) %></td>
                <td>$<%= item.Discounts.toFixed(2) %></td>
                <td>$<%= item.NetSales.toFixed(2) %></td>
                <td>$<%= item.Tax.toFixed(2) %></td>
                <td>$<%= item.CreditCard.toFixed(2) %></td>
                <td>$<%= item.DebitCard.toFixed(2) %></td>
                <td>$<%= item.Cash.toFixed(2) %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
